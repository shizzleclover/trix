{
    "name": "vue-router",
    "type": "routing",
    "files": [
        {
            "source": "frontend/modules/routing/vue-router/index.ts",
            "destination": "src/router/index.{{#if typescript}}ts{{else}}js{{/if}}",
            "type": "template"
        }
    ],
    "dependencies": {
        "prod": {
            "vue-router": "^4.2.5"
        },
        "dev": {}},
    "injections": [
        {
            "file": "src/main.ts",
            "position": "import",
            "code": "import router from './router';"
        },
        {
            "file": "src/main.ts",
            "position": "custom",
            "marker": "createApp(App)",
            "code": ".use(router)"
        }
    ]
}