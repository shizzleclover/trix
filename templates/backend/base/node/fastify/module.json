{
    "name": "node-fastify",
    "type": "base",
    "framework": "fastify",
    "runtime": "node",
    "files": [
        {
            "source": "backend/base/node/fastify/src/app.ts",
            "destination": "src/app.{{#if typescript}}ts{{else}}js{{/if}}",
            "type": "template"
        },
        {
            "source": "backend/base/node/fastify/src/routes/root.ts",
            "destination": "src/routes/root.{{#if typescript}}ts{{else}}js{{/if}}",
            "type": "template"
        },
        {
            "source": "backend/base/node/fastify/src/server.ts",
            "destination": "src/server.{{#if typescript}}ts{{else}}js{{/if}}",
            "type": "template"
        }
    ],
    "dependencies": {
        "prod": {
            "fastify": "^4.25.1",
            "@fastify/cors": "^8.5.0",
            "@fastify/autoload": "^5.8.0",
            "@fastify/sensible": "^5.5.0",
            "dotenv": "^16.3.1"
        },
        "dev": {
            "@types/node": "^20.10.5",
            "tsx": "^4.7.0"}},
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js"}}