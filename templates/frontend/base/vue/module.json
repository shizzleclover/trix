{
    "name": "vue-base",
    "type": "base",
    "framework": "vue",
    "files": [
        {
            "source": "frontend/base/vue/src/App.vue",
            "destination": "src/App.vue",
            "type": "template"
        },
        {
            "source": "frontend/base/vue/src/main.ts",
            "destination": "src/main.{{#if typescript}}ts{{else}}js{{/if}}",
            "type": "template"
        },
        {
            "source": "frontend/base/vue/index.html",
            "destination": "index.html",
            "type": "template"
        },
        {
            "source": "frontend/base/vue/vite.config.ts",
            "destination": "vite.config.{{#if typescript}}ts{{else}}js{{/if}}",
            "type": "template"
        }
    ],
    "dependencies": {
        "prod": {
            "vue": "^3.3.11"
        },
        "dev": {
            "@vitejs/plugin-vue": "^4.5.2",
            "vite": "^5.0.8",
            "@types/node": "^20.10.5"}},
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview"}}